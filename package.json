{
  "name": "kanon",
  "description": "MonoRepo for Kanon app",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "libs/*"
  ],
  "scripts": {
    "dev:web": "yarn workspace @kanon/web dev",
    "dev:api": "yarn workspace @kanon/api dev",
    "test:web": "yarn workspace @kanon/web test",
    "test:api": "yarn workspaces @kanon/api test",
    "lint:web": "yarn workspace @kanon/web lint",
    "lint:api": "yarn workspaces @kanon/api lint",
    "test": "yarn workspaces run test",
    "lint": "yarn workspaces run lint"
  },
  "dependencies": {
    "pino-pretty": "^13.1.1",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.10.0"
  }
}
